{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Logical Operators Schema",
  "properties": {
    "anyOfExample": {
      "anyOf": [
        {
          "type": "string",
          "minLength": 5
        },
        {
          "type": "number",
          "minimum": 10
        },
        {
          "type": "object",
          "properties": {
            "code": {
              "type": "string"
            }
          },
          "required": [
            "code"
          ]
        }
      ]
    },
    "allOfExample": {
      "allOf": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            }
          },
          "required": [
            "id"
          ]
        },
        {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            }
          },
          "required": [
            "name"
          ]
        },
        {
          "type": "object",
          "properties": {
            "age": {
              "type": "number",
              "minimum": 0
            }
          },
          "required": [
            "age"
          ]
        }
      ]
    },
    "oneOfExample": {
      "oneOf": [
        {
          "type": "object",
          "properties": {
            "type": {
              "const": "circle"
            },
            "radius": {
              "type": "number",
              "minimum": 0
            }
          },
          "required": [
            "type",
            "radius"
          ],
          "additionalProperties": false
        },
        {
          "type": "object",
          "properties": {
            "type": {
              "const": "rectangle"
            },
            "width": {
              "type": "number",
              "minimum": 0
            },
            "height": {
              "type": "number",
              "minimum": 0
            }
          },
          "required": [
            "type",
            "width",
            "height"
          ],
          "additionalProperties": false
        }
      ]
    },
    "notExample": {
      "not": {
        "type": "object",
        "properties": {
          "forbidden": {
            "type": "string"
          },
          "status": {
            "const": "inactive"
          }
        },
        "required": [
          "forbidden",
          "status"
        ]
      }
    },
    "combinedExample": {
      "type": "object",
      "properties": {
        "value": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "number"
            }
          ]
        },
        "metadata": {
          "allOf": [
            {
              "type": "object",
              "properties": {
                "created": {
                  "type": "string",
                  "format": "date-time"
                }
              },
              "required": [
                "created"
              ]
            },
            {
              "not": {
                "type": "object",
                "properties": {
                  "deleted": {
                    "const": true
                  }
                }
              }
            }
          ]
        }
      },
      "required": [
        "value",
        "metadata"
      ]
    }
  },
  "required": [
    "anyOfExample",
    "allOfExample",
    "oneOfExample",
    "notExample",
    "combinedExample"
  ]
}